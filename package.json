{
  "name": "bookshelf-react-express-sqlite-app",
  "version": "1.0.0",
  "description": "App for collecting books built with React, Express and SQLite.",
  "author": {
    "name": "Claudio Vigliarolo"
  },
  "homepage": "/",
  "private": false,
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0"
  },
  "main": "src/index.tsx",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "dev": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "start": "node server/server.js",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "bootstrap": "^4.6.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "morgan": "^1.10.0",
    "react": "16.13.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.6",
    "serve": "^11.3.2",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-window": "^1.8.2",
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "^3.4.4",
    "sqlite3": "4.1.1",
    "typescript": "3.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
